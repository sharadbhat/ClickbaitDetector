{
    "manifest_version": 2,
    "name": "Clickbait Detector",
    "version": "0.1",
    "author": "Project Group B-7 ISE PESIT-BSC",
    "description": "Calculates probability of an article headline being clickbait.",
    "browser_action": {
      "default_icon": {
        "200": "icons/clickbait-200.png"
      },
      "default_title": "Clickbait Detector",
      "default_popup": "popup/popup.html"
    },
    "icons": {
      "200": "icons/clickbait-200.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js":["cb_content.js"]
      }
    ],
    "background": {
      "scripts": ["cb_background.js"],
    },
    "permissions": ["activeTab", "tabs", "webRequest", "http://localhost/*"]
  }